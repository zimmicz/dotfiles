\timing on
\set AUTOCOMMIT false
\set PROMPT1 '%m->%n@%/%# '
\set PROMPT2 '[more] %R > '
\pset null '[NULL]'
\x auto
\set COMP_KEYWORD_CASE upper
\set HISTFILE ~/.psql_history- :DBNAME
\set PSQL_EDITOR vim
\set HISTCONTROL ignorespace
\set HISTSIZE 20000
\pset linestyle unicode
\pset border 2
\set x '\\setenv PAGER less'
\set xx '\\setenv PAGER \'pspg -bX --no-mouse --IGNORE-CASE --no-sound -F\''
