#!/bin/bash

SESSION=gmt-maja

tmux new-session -d -s $SESSION

# run backend
tmux split-window -h
tmux send-keys "cd ~/documents/gmt-symap-frontend" C-m
tmux send-keys "yarn run start-dev" C-m
tmux resize-pane -R 50

tmux select-pane -t 1
tmux send-keys "cd ~/documents/gmt-symap-frontend" C-m
tmux send-keys "vim -S Session.vim" C-m

